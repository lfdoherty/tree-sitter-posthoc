{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "posthoc",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_top_level_statement"
      }
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\/\\/[^\\n]*"
      }
    },
    "block_comment": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\/\\*(.|\\n)*\\*\\/"
      }
    },
    "_top_level_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "function_definition"
        },
        {
          "type": "SYMBOL",
          "name": "assignment"
        },
        {
          "type": "SYMBOL",
          "name": "include"
        }
      ]
    },
    "include": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "!include "
        },
        {
          "type": "PATTERN",
          "value": "[a-z\\-\\_\\.\\/]+"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "return_if": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "return-if"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "SYMBOL",
          "name": "_expr"
        },
        {
          "type": "SYMBOL",
          "name": "_expr"
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    },
    "assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "names",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "assignment_name"
            }
          }
        },
        {
          "type": "STRING",
          "value": ": "
        },
        {
          "type": "FIELD",
          "name": "expr",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "identifier"
              },
              {
                "type": "SYMBOL",
                "name": "compound_identifier"
              },
              {
                "type": "SYMBOL",
                "name": "number"
              },
              {
                "type": "SYMBOL",
                "name": "string"
              },
              {
                "type": "SYMBOL",
                "name": "multiline_call"
              },
              {
                "type": "SYMBOL",
                "name": "base_single_line_call"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_newline"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "assignment_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "|"
                },
                {
                  "type": "FIELD",
                  "name": "type",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_single_line_expr"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "function_parameter": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "parameter_name"
          }
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "|"
            },
            {
              "type": "FIELD",
              "name": "type",
              "content": {
                "type": "SYMBOL",
                "name": "_single_line_expr"
              }
            }
          ]
        }
      ]
    },
    "function_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "supertype",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ":"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "identifier"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "/"
            },
            {
              "type": "FIELD",
              "name": "return_type",
              "content": {
                "type": "SYMBOL",
                "name": "_single_line_expr"
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "FIELD",
          "name": "parameters",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "function_parameter"
                },
                {
                  "type": "STRING",
                  "value": " "
                }
              ]
            }
          }
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "=>"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_newline"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_indent"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "block"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_dedent"
                  }
                ]
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "=> "
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "SYMBOL",
                            "name": "identifier"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "_newline"
                          }
                        ]
                      },
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "SYMBOL",
                            "name": "compound_identifier"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "_newline"
                          }
                        ]
                      },
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "SYMBOL",
                            "name": "number"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "_newline"
                          }
                        ]
                      },
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "SYMBOL",
                            "name": "string"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "_newline"
                          }
                        ]
                      },
                      {
                        "type": "SYMBOL",
                        "name": "multiline_call"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "base_single_line_call"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "lambda_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "::"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": " "
              },
              {
                "type": "SYMBOL",
                "name": "parameter_name"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "assignment"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "return_if"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_expr"
        }
      ]
    },
    "multiline_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "FIELD",
          "name": "arguments",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_expr"
                }
              ]
            }
          }
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    },
    "base_single_line_call": {
      "type": "PREC",
      "value": 9,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            }
          },
          {
            "type": "FIELD",
            "name": "arguments",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": " "
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_single_line_expr"
                      }
                    ]
                  }
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": " "
                        },
                        {
                          "type": "CHOICE",
                          "members": [
                            {
                              "type": "SEQ",
                              "members": [
                                {
                                  "type": "SYMBOL",
                                  "name": "identifier"
                                },
                                {
                                  "type": "SYMBOL",
                                  "name": "_newline"
                                }
                              ]
                            },
                            {
                              "type": "SEQ",
                              "members": [
                                {
                                  "type": "SYMBOL",
                                  "name": "compound_identifier"
                                },
                                {
                                  "type": "SYMBOL",
                                  "name": "_newline"
                                }
                              ]
                            },
                            {
                              "type": "SEQ",
                              "members": [
                                {
                                  "type": "SYMBOL",
                                  "name": "number"
                                },
                                {
                                  "type": "SYMBOL",
                                  "name": "_newline"
                                }
                              ]
                            },
                            {
                              "type": "SEQ",
                              "members": [
                                {
                                  "type": "SYMBOL",
                                  "name": "string"
                                },
                                {
                                  "type": "SYMBOL",
                                  "name": "_newline"
                                }
                              ]
                            },
                            {
                              "type": "SYMBOL",
                              "name": "multiline_call"
                            },
                            {
                              "type": "SYMBOL",
                              "name": "lambda_definition"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            }
          }
        ]
      }
    },
    "single_line_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "arguments",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_single_line_expr"
                  },
                  {
                    "type": "REPEAT",
                    "content": {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": " "
                        },
                        {
                          "type": "SYMBOL",
                          "name": "_single_line_expr"
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "compound_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "multiline_call"
        },
        {
          "type": "SYMBOL",
          "name": "lambda_definition"
        },
        {
          "type": "SYMBOL",
          "name": "base_single_line_call"
        }
      ]
    },
    "_single_line_expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "compound_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "single_line_call"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z&][a-z\\-0-9]*"
    },
    "parameter_name": {
      "type": "PATTERN",
      "value": "[a-z\\+][a-z\\-0-9]*"
    },
    "compound_identifier": {
      "type": "PATTERN",
      "value": "[a-z][a-z\\-0-9\\.]*"
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\d+"
        },
        {
          "type": "PATTERN",
          "value": "\\-\\d+"
        }
      ]
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "PATTERN",
          "value": "[^']*"
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "line_comment"
    },
    {
      "type": "PATTERN",
      "value": "[\\s\\f]|\\r"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_newline"
    },
    {
      "type": "SYMBOL",
      "name": "_indent"
    },
    {
      "type": "SYMBOL",
      "name": "_dedent"
    }
  ],
  "inline": [],
  "supertypes": []
}
